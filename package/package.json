{
  "name": "shell-parser",
  "version": "0.0.0",
  "author": "Rafa Mel",
  "description": "Shell parser",
  "keywords": [
    "keyword",
    "keyword",
    "keyword"
  ],
  "scripts": {
    "prepublishOnly": "exit 1",
    "start": "npm run watch",
    "build": "nps build",
    "build:prepare": "nps build.prepare",
    "build:transpile": "nps build.transpile",
    "build:declaration": "nps build.declaration",
    "build:promote": "nps build.promote",
    "watch": "nps watch",
    "fix": "nps fix",
    "types": "nps types",
    "lint": "nps lint",
    "lint:scripts": "nps lint.scripts",
    "test": "nps test",
    "test:watch": "nps test.watch",
    "validate": "nps validate",
    "update": "nps update",
    "clean": "nps clean",
    "@root": "cd ../ && npm run",
    "@sh": "npm run @root -- @sh",
    "@core": "npm run @root -- @core",
    "@package": "npm run @root -- @package",
    "@all": "npm run @root -- @all",
    ":commit": "npm run @root -- :commit --",
    ":version": "npm run @root -- :version --"
  },
  "license": "MIT",
  "homepage": "https://github.com/rafamel/shell-parser",
  "repository": {
    "type": "git",
    "url": "https://github.com/rafamel/shell-parser"
  },
  "bugs": {
    "url": "https://github.com/rafamel/shell-parser/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@types/camelcase": "^4.1.0",
    "@types/uuid": "^3.4.4",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.14.1",
    "jake": "^8.0.19",
    "jest-cli": "^24.0.0",
    "nps": "^5.9.4",
    "onchange": "^5.2.0",
    "prettier": "^1.16.4",
    "shx": "^0.3.2",
    "ttypescript": "^1.5.6",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "camelcase": "^5.0.0",
    "uuid": "^3.3.2"
  }
}
